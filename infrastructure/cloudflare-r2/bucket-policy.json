{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadForLessons",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::lms-files/lessons/*"
    },
    {
      "Sid": "PublicReadForAvatars",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::lms-files/users/*/avatar/*"
    }
  ]
}
